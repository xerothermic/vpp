create interface memif id 0 master
create loopback interface mac de:ad:00:00:00:01
create host-interface name vpp1out

set int state memif0/0 up
set int state loop0 up
set int state host-vpp1out up

set int ip address loop0 192.168.55.4/24

set int l2 bridge loop0 1 bvi
set int l2 bridge memif0/0 1
set int l2 bridge host-vpp1out 1

ip route add 0.0.0.0/0 via 192.168.55.1
